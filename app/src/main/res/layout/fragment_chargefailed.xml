<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="@dimen/width_chargefailed_linearlayout"
        android:layout_height="@dimen/height_chargefailed_linearlayout"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margintop_chargefailed_linearlayout"
        android:background="@drawable/chargefailed_border"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageview_chargefailed_failed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/margintop_chargefailed_failedimageview"
            android:src="@drawable/chargefailed_failed" />

        <TextView
            android:id="@+id/textview_chargefailed_success"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_chargefailed_failedtextview"
            android:gravity="center"
            android:text="@string/chargefailed_failedtextview"
            android:textColor="@color/black"
            android:textSize="@dimen/textsize_chargefailed_failedtextview" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            >

            <TextView
                android:id="@+id/textview_chargefailed_walletnamelable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margintop_chargefailed_walletnamelable"
                android:text="@string/chargefailed_walletname"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_walletnamelable" />

            <TextView
                android:id="@+id/textview_chargefailed_walletname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margintop_chargefailed_walletname"
                android:layout_toRightOf="@+id/textview_chargefailed_walletnamelable"
                android:paddingStart="@dimen/paddingstart_chargefailed_walletname"
                android:text="VALUE"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_walletnamelable" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="@dimen/margintop_chargefailed_relativelayout">

            <TextView
                android:id="@+id/textview_chargefailed_amountalphalable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/chargefailed_walletamount"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_walletnolable" />

            <TextView
                android:id="@+id/textview_chargefailed_amountalpha"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textview_chargefailed_amountalphalable"
                android:paddingStart="@dimen/paddingstart_chargefailed_walletno"
                android:text="VALUE"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_walletnolable" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="@dimen/margintop_chargefailed_relativelayout">

            <TextView
                android:id="@+id/textview_chargefailed_amountdollarlable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/chargefailed_baseamount"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_amountdollarlable" />

            <TextView
                android:id="@+id/textview_chargefailed_amountdollar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textview_chargefailed_amountdollarlable"
                android:paddingStart="@dimen/paddingstart_chargefailed_amountalpha"
                android:text="VALUE"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_amountdollarlable" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="@dimen/margintop_chargefailed_relativelayout">

            <TextView
                android:id="@+id/textview_chargefailed_transactionamountlable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/chargefailed_transactionamount"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_amountdollarlable" />

            <TextView
                android:id="@+id/textview_chargefailed_transactionamount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textview_chargefailed_transactionamountlable"
                android:paddingStart="@dimen/paddingstart_chargefailed_amountalpha"
                android:text="VALUE"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_amountdollarlable" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="@dimen/margintop_chargefailed_relativelayout">

            <TextView
                android:id="@+id/textview_chargefailed_paymentgatewaylable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/chargefailed_paymentgateway"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_paymentgatewaylable" />

            <TextView
                android:id="@+id/textview_chargefailed_paymentgateway"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textview_chargefailed_paymentgatewaylable"
                android:paddingStart="@dimen/paddingstart_chargefailed_amountdollar"
                android:text="VALUE"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_paymentgatewaylable" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="@dimen/margintop_chargefailed_relativelayout">

            <TextView
                android:id="@+id/textview_chargefailed_transactiondatelable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/chargefailed_transactiondate"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_transactiondate" />

            <TextView
                android:id="@+id/textview_chargefailed_transactiondate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textview_chargefailed_transactiondatelable"
                android:paddingStart="@dimen/paddingstart_chargefailed_transactiondate"
                android:text="VALUE"
                android:textColor="@color/black"
                android:textSize="@dimen/textsize_chargefailed_transactiondate" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativelayout_chargefailed_consistdownloadandshare"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/marginbutton_chargefailed_relativelayoutconsistdownloadandshare"
            android:layout_marginTop="@dimen/margintop_chargefailed_relativelayoutconsistdownloadandshare">

            <ImageButton
                android:id="@+id/imagebutton_chargefailed_sharebutton"
                android:layout_width="@dimen/widthheight_chargefailed_sharebutton"
                android:layout_height="@dimen/widthheight_chargefailed_sharebutton"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="@dimen/marginstartend_chargefailed_downloadsharebutton"
                android:src="@drawable/chargecompletedfailed_sharebutton"

                />

            <ImageButton
                android:id="@+id/imagebutton_chargefailed_downloadbutton"
                android:layout_width="@dimen/widthheight_chargefailed_downloadbutton"
                android:layout_height="@dimen/widthheight_chargefailed_downloadbutton"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="@dimen/marginstartend_chargefailed_downloadsharebutton"
                android:src="@drawable/chargecompletedfailed_downloadbutton" />


        </RelativeLayout>

    </LinearLayout>

    <Button
        android:id="@+id/button_chargefailed_dashborad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/marginbuttom_chargefailed_dashboard"
        android:layout_marginEnd="@dimen/marginstartend_chargefailed_dashboard"
        android:layout_marginStart="@dimen/marginstartend_chargefailed_dashboard"
        android:background="@color/colorSecondary"
        android:text="@string/chargefailed_dashboard"
        android:textColor="@color/black"
        android:textSize="@dimen/textsize_chargefailed_dashboardbutton" />
</RelativeLayout>