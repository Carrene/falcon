<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <LinearLayout
        android:id="@+id/linearlayout_exchangecompleted_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/marginstart_exchangecompleted_linearlayoutmain"
        android:layout_marginTop="@dimen/margintop_exchangecompleted_linearlayoutmain"
        android:layout_marginEnd="@dimen/marginend_exchangecompleted_linearlayoutmain"
        android:background="@drawable/loadsendconfirmation_border"
        android:orientation="vertical"
        android:paddingBottom="@dimen/paddingbottom_exchangecompleted_linearlayoutmain">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_realativelayout"
            android:background="@drawable/loadsendcompleted_headerborder">

            <TextView
                android:id="@+id/textview_exchangecompleted_success"
                android:layout_width="match_parent"
                android:layout_height="@dimen/height_exchangecompleted_textviewsuccess"
                android:gravity="center"
                android:text="Success"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="@dimen/textsize_exchangecompleted_textviewsuccess" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_walletnamelable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/exchangecompleted_walletname"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewamountlabel" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textview_exchangecompleted_walletname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewamount"
                    android:text="@string/text_exchangecompleted_textviewwalletname"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textsize_exchangecompleted_textviewamount" />
            </LinearLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_view"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_view"
            android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_view"
            android:background="@color/colorPrimaryDark5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_paidamountlabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_exchangecompleted_textviewpaidamountlable"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewpaidamountlable" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textview_exchangecompleted_paidamount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewpaidamount"
                    android:text="@string/text_exchangecompleted_textviewpaidamount"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textsize_exchangecompleted_textviewpaidamount" />
            </LinearLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_view"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_view"
            android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_view"
            android:background="@color/colorPrimaryDark5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_receivedamountlabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_exchangecompleted_textviewreceivedamountlable"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewpaidamountlable" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textview_exchangecompleted_receivedamount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewpaidamount"
                    android:text="@string/text_exchangecompleted_textviewpaidamount"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/textsize_exchangecompleted_textviewpaidamount" />
            </LinearLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_view"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_view"
            android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_view"
            android:background="@color/colorPrimaryDark5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_exchangeratelabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_exchangecompleted_textviewexchangeratelabel"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewdatelabel" />

            <TextView
                android:id="@+id/textview_exchangecompleted_exchangerate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewdate"
                android:text="@string/text_exchangecompleted_textviewdate"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewdate" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_view"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_view"
            android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_view"
            android:background="@color/colorPrimaryDark5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_transactiondatelabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_exchangecompleted_textviewdatelabel"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewdatelabel" />

            <TextView
                android:id="@+id/textview_exchangecompleted_transactiondate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewdate"
                android:text="@string/text_exchangecompleted_textviewdate"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewdate" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_view"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_view"
            android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_view"
            android:background="@color/colorPrimaryDark5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_transactiontimelabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_exchangecompleted_textviewtimelabel"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewtimelabel" />

            <TextView
                android:id="@+id/textview_exchangecompleted_transactiontime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewdate"
                android:text="VALUE"
                android:textColor="@color/colorBlack"
                android:textSize="14sp" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_exchangecompleted_view"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_view"
            android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_view"
            android:background="@color/colorPrimaryDark5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margintop_exchangecompleted_relativelayout"
            android:paddingStart="@dimen/paddingstart_exchangecompleted_relativelayout">

            <TextView
                android:id="@+id/textview_exchangecompleted_rrnlabel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_exchangecompleted_textviewrnnlabel"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewrnnlabel" />

            <TextView
                android:id="@+id/textview_exchangecompleted_rrn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:paddingEnd="@dimen/paddingend_exchangecompleted_textviewrnn"
                android:text="@string/text_exchangecompleted_textviewrnn"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/textsize_exchangecompleted_textviewrnn" />
        </RelativeLayout>
    </LinearLayout>

    <Button
        android:id="@+id/button_exchangecompleted_dashborad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/marginstart_exchangecompleted_buttondashboard"
        android:layout_marginEnd="@dimen/marginend_exchangecompleted_buttondashboard"
        android:layout_marginBottom="@dimen/marginbottom_exchangecompleted_buttondashboard"
        android:background="@color/colorSecondary"
        android:text="@string/sendcompleted_dashboard"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/textsize_exchangecompleted_buttondashboard" />
</RelativeLayout>